{
  "name": "arka-acres",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/startup-check.js && next dev",
    "build": "prisma generate && bash scripts/resolve-migration-inline.sh && prisma migrate deploy && next build",
    "migrate:resolve": "prisma migrate resolve --applied",
    "migrate:fix-vercel": "node scripts/resolve-vercel-migration.js",
    "migrate:auto-resolve": "bash scripts/auto-resolve-migration.sh",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules/.cache",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "postinstall": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset && npm run db:seed",
    "db:push": "prisma db push && npm run db:seed",
    "verify:config": "tsx scripts/verify-config.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.4",
    "bcryptjs": "^3.0.3",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.294.0",
    "next": "^14.0.4",
    "nodemailer": "^7.0.12",
    "prisma": "^7.2.0",
    "react": "^18.2.0",
    "react-day-picker": "^9.13.0",
    "react-dom": "^18.2.0",
    "resend": "^6.6.0",
    "twilio": "^5.11.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vercel": "^50.1.3"
  }
}
